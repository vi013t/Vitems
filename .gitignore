/VitemsAssets/*.csproj
/VitemsAssets/*.unityproj
/VitemsAssets/*.sln
/VitemsAssets/*.suo
/VitemsAssets/*.tmp
/VitemsAssets/*.user
/VitemsAssets/*.userprefs
/VitemsAssets/*.pidb
/VitemsAssets/*.booproj
/VitemsAssets/*.svd
/VitemsAssets/*.pdb
/VitemsAssets/*.mdb
/VitemsAssets/*.opendb
/VitemsAssets/*.VC.db

# Unity3D generated meta files
/VitemsAssets/*.pidb.meta
/VitemsAssets/*.pdb.meta
/VitemsAssets/*.mdb.meta

# Unity3D generated file on crash reports
/VitemsAssets/sysinfo.txt

# Builds
/VitemsAssets/*.apk
/VitemsAssets/*.aab
/VitemsAssets/*.unitypackage
/VitemsAssets/*.app

# Crashlytics generated file
/VitemsAssets/crashlytics-build.properties

